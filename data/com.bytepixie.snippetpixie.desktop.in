[Desktop Entry]
Name=Snippet Pixie
Comment=Your little snippet helper
Exec=com.bytepixie.snippetpixie
Icon=com.bytepixie.snippetpixie
Keywords=expand;text;snippet;
Terminal=false
Type=Application
StartupNotify=true
Categories=GTK;Utility;
Actions=Show;Quit;

[Desktop Action Show]
Name=Show Snippet Pixie
Exec=com.bytepixie.snippetpixie --show

[Desktop Action Quit]
Name=Quit Snippet Pixie
Exec=com.bytepixie.snippetpixie --quit
